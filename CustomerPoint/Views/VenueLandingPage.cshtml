@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
    Layout = "Site.cshtml";
}
<div class="row">
    <div class="col-md-12">
        <div class="page-header">
            <h1>@Umbraco.Field("title")</h1>
        </div>
        @Umbraco.Field("bodyText")
    </div>
</div>
<div class="row">
	<div class="col-md-12">
		<div class="venues">
			@Html.Partial("VenueListBlocks")
		</div>
	</div>
</div>
@Html.Partial( 
    "LandingPageBlock", 
    Model.Content,
    new ViewDataDictionary()
    {
        { "mainCount", @Umbraco.Field("mainItemCount")},
		{ "ignoreType", "VenuePage" }
    })